{
  "files.associations": {
    ".aliases": "shellscript",
    "**/zfuncs/*": "shellscript",
    "**/lib/*": "shellscript",
    "lib-includes": "shellscript",
    ".*rc": "shellscript",
    ".z*": "shellscript",

    "zsh.plugins/*": "shellscript",
    "zsh.custom/*": "shellscript",
    "async": "shellscript",
    "prompt_pure_*": "shellscript",

    ".*vim": "viml",
    ".*vimrc": "viml",
  },
  // "git.detectSubmodulesLimit": 20,
  "git.detectSubmodules": false,
  "shellcheck.enable": false,
  "python.formatting.provider": "black",
  "python.pythonPath": "~/.pyenv/versions/3.8.0/bin/python",
}
